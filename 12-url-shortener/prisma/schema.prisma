// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

model UrlMap {
  id     String     @id @default(uuid())
  url    String
  short  String
  hash   String 
}

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_PRISMA_URL")
}
